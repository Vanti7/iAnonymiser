{
    "id": "kubernetes",
    "name": "Kubernetes",
    "description": "Logs Kubernetes et Docker",
    "patterns": [
        "ipv4", "hostname", "uuid", "path_unix", "email", "server_name"
    ],
    "preserve": ["localhost", "kubernetes.default"],
    "custom_patterns": [
        {
            "regex": "pod/[a-z0-9-]+",
            "prefix": "POD"
        },
        {
            "regex": "namespace/[a-z0-9-]+",
            "prefix": "NS"
        },
        {
            "regex": "[a-z0-9]+-[a-z0-9]+-[a-z0-9]+-[a-z0-9]+-[a-z0-9]+",
            "prefix": "KUID"
        }
    ]
}

